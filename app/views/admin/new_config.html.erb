<h1>Add Config Key</h1>
<%= form_for(@config, :url => create_config_path) do |f| %>
  <%= render 'shared/error_messages', :object => f.object %>
  <div class="field">
    <%= f.label :key %><br />
    <%=f.text_field :key %>
  </div>
  <div class="field">
    <%= f.label :value %><br />
    <%=f.text_area :value %>
  </div>
  <div class="actions">
    <%= f.submit "Create" %>
  </div>
<% end %>